<!DOCTYPE html>
<html>
  <head>
    <title>GoogleFlights</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <link href="/assets/favicon.ico" rel="shortcut icon" type="image/x-icon" />

  </head>

  <body>
    <%= link_to 'Sign Out',  destroy_user_session_path, :method => "delete" if user_signed_in? %>
    <%= link_to "Sign In", new_user_session_path if !user_signed_in? %>
    <%= link_to "Sign Up", new_user_session_path if !user_signed_in? %>
    <%= yield %>
  </body>
</html>
